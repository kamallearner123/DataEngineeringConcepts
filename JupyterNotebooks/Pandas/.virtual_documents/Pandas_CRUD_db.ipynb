import sqlite3

# Create SQLite database and table
conn = sqlite3.connect('sales1.db')
cursor = conn.cursor()
cursor.execute('''
    CREATE TABLE IF NOT EXISTS sales (
        id INTEGER PRIMARY KEY AUTOINCREMENT,
        product TEXT,
        amount REAL,
        sale_date TEXT
    )
''')
conn.commit()
conn.close()

